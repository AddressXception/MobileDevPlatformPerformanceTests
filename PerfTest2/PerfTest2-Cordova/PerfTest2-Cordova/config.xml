<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.PerfTest2Cordova" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>PerfTest2-Cordova</name>
  <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
  <author href="http://windingroadway.blogspot.com/" email="kevinf@magenic.com">Kevin E. Ford</author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="android-minSdkVersion" value="17" />
  <preference name="android-targetSdkVersion" value="17" />
  <preference name="Fullscreen" value="true" />
  <vs:plugin name="com.brodysoft.sqlitePlugin" version="1.0.3" />
  <vs:plugin name="org.apache.cordova.file" version="1.3.1" />
  <vs:platformSpecificValues />
</widget>